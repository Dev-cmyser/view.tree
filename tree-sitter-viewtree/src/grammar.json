{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "viewtree",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "blank"
          },
          {
            "type": "SYMBOL",
            "name": "component"
          }
        ]
      }
    },
    "blank": {
      "type": "SYMBOL",
      "name": "_newline"
    },
    "component": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "component_name"
            }
          },
          {
            "type": "FIELD",
            "name": "base",
            "content": {
              "type": "SYMBOL",
              "name": "component_name"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "property_list"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "component_name": {
      "type": "PATTERN",
      "value": "\\$[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "property_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "blank"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "_eqindent"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "comment_node"
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "_eqindent"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "node"
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "_eqindent"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "raw_line"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "comment_node": {
      "type": "PREC_DYNAMIC",
      "value": 10,
      "content": {
        "type": "PREC_RIGHT",
        "value": 2,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "inline_value"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "property_list"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      }
    },
    "inline_value": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "value"
        }
      }
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "raw_string"
        },
        {
          "type": "SYMBOL",
          "name": "localized_string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "special_number"
        },
        {
          "type": "SYMBOL",
          "name": "component_name"
        }
      ]
    },
    "localized_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "raw_string"
        }
      ]
    },
    "node": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "path",
            "content": {
              "type": "SYMBOL",
              "name": "node_path"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "property_list"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "raw_line": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "raw",
            "content": {
              "type": "SYMBOL",
              "name": "raw_string"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "node_path": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "path_element"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": " "
              },
              {
                "type": "SYMBOL",
                "name": "path_element"
              }
            ]
          }
        }
      ]
    },
    "path_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "arrow_both"
        },
        {
          "type": "SYMBOL",
          "name": "arrow_left"
        },
        {
          "type": "SYMBOL",
          "name": "arrow_right"
        },
        {
          "type": "SYMBOL",
          "name": "op_dash"
        },
        {
          "type": "SYMBOL",
          "name": "op_at"
        },
        {
          "type": "SYMBOL",
          "name": "op_caret"
        },
        {
          "type": "SYMBOL",
          "name": "typed_list"
        },
        {
          "type": "SYMBOL",
          "name": "op_slash"
        },
        {
          "type": "SYMBOL",
          "name": "op_star"
        },
        {
          "type": "SYMBOL",
          "name": "raw_string"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "special_number"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "component_name"
        },
        {
          "type": "SYMBOL",
          "name": "property_identifier"
        }
      ]
    },
    "arrow_both": {
      "type": "STRING",
      "value": "<=>"
    },
    "arrow_left": {
      "type": "STRING",
      "value": "<="
    },
    "arrow_right": {
      "type": "STRING",
      "value": "=>"
    },
    "op_dash": {
      "type": "STRING",
      "value": "-"
    },
    "op_slash": {
      "type": "STRING",
      "value": "/"
    },
    "op_star": {
      "type": "STRING",
      "value": "*"
    },
    "op_caret": {
      "type": "STRING",
      "value": "^"
    },
    "op_at": {
      "type": "STRING",
      "value": "@"
    },
    "typed_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "component_name"
            },
            {
              "type": "SYMBOL",
              "name": "type_name"
            }
          ]
        }
      ]
    },
    "type_name": {
      "type": "PATTERN",
      "value": "string|number|boolean"
    },
    "raw_string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "null": {
      "type": "STRING",
      "value": "null"
    },
    "special_number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "NaN"
        },
        {
          "type": "STRING",
          "value": "Infinity"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "STRING",
              "value": "Infinity"
            }
          ]
        }
      ]
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "+"
                  },
                  {
                    "type": "STRING",
                    "value": "-"
