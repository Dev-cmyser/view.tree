{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "viewtree",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "token_chain"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "children"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "children": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "line"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "token_chain": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_token"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " "
                },
                {
                  "type": "SYMBOL",
                  "name": "token_chain"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_token": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "component_name"
        },
        {
          "type": "SYMBOL",
          "name": "binding_operator"
        },
        {
          "type": "SYMBOL",
          "name": "comment_marker"
        },
        {
          "type": "SYMBOL",
          "name": "list_marker"
        },
        {
          "type": "SYMBOL",
          "name": "dict_marker"
        },
        {
          "type": "SYMBOL",
          "name": "localization_marker"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "component_name": {
      "type": "PATTERN",
      "value": "\\$[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "binding_operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "<=>"
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": "=>"
        }
      ]
    },
    "comment_marker": {
      "type": "STRING",
      "value": "-"
    },
    "list_marker": {
      "type": "STRING",
      "value": "/"
    },
    "dict_marker": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "^"
        }
      ]
    },
    "localization_marker": {
      "type": "STRING",
      "value": "@"
    },
    "string_literal": {
      "type": "PATTERN",
      "value": "\\\\[^\\n\\r]*"
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[+-]?\\d+\\.\\d+"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?\\d+"
        },
        {
          "type": "STRING",
          "value": "NaN"
        },
        {
          "type": "STRING",
          "value": "Infinity"
        },
        {
          "type": "STRING",
          "value": "-Infinity"
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "null": {
      "type": "STRING",
      "value": "null"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*[?!*]?"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\t"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    },
    {
      "type": "SYMBOL",
      "name": "_newline"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}