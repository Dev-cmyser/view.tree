                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "STRING",
                            "value": "?"
                          },
                          {
                            "type": "STRING",
                            "value": "!"
                          },
                          {
                            "type": "STRING",
                            "value": "*"
                          }
                        ]
                      },
                      {
                        "type": "PATTERN",
                        "value": "[a-zA-Z0-9_]+"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
