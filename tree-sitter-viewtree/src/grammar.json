{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "viewtree",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "children"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "children": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "line"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "node": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "token"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": " "
              },
              {
                "type": "SYMBOL",
                "name": "token"
              }
            ]
          }
        }
      ]
    },
    "token": {
      "type": "PATTERN",
      "value": "[^\\t\\n\\r ]+"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\t"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    },
    {
      "type": "SYMBOL",
      "name": "_newline"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}