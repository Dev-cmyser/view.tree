{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "viewtree",
  "rules": {
    "source_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "indent"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "node_path"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "node_path": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "sep"
              },
              {
                "type": "SYMBOL",
                "name": "node"
              }
            ]
          }
        }
      ]
    },
    "node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "raw_string"
        },
        {
          "type": "SYMBOL",
          "name": "arrow_both"
        },
        {
          "type": "SYMBOL",
          "name": "arrow_left"
        },
        {
          "type": "SYMBOL",
          "name": "arrow_right"
        },
        {
          "type": "SYMBOL",
          "name": "special_dash"
        },
        {
          "type": "SYMBOL",
          "name": "special_slash"
        },
        {
          "type": "SYMBOL",
          "name": "special_star"
        },
        {
          "type": "SYMBOL",
          "name": "special_caret"
        },
        {
          "type": "SYMBOL",
          "name": "special_at"
        },
        {
          "type": "SYMBOL",
          "name": "ident"
        }
      ]
    },
    "indent": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\t+"
      }
    },
    "sep": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": " "
      }
    },
    "newline": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "\n"
      }
    },
    "arrow_both": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "<=>"
      }
    },
    "arrow_left": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "<="
      }
    },
    "arrow_right": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "=>"
      }
    },
    "special_dash": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "-"
      }
    },
    "special_slash": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "/"
      }
    },
    "special_star": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "*"
      }
    },
    "special_caret": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "^"
      }
    },
    "special_at": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "@"
      }
    },
    "ident": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[\\$A-Za-z_][\\w\\.\\$]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "*"
                  },
                  {
                    "type": "STRING",
                    "value": "?"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "raw_string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}