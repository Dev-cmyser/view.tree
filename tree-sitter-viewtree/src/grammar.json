{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "viewtree",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "commented_line"
        },
        {
          "type": "SYMBOL",
          "name": "normal_line"
        }
      ]
    },
    "commented_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_marker"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "commented_children"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment_content": {
      "type": "PATTERN",
      "value": "[^\\n\\r]+"
    },
    "commented_children": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "commented_child_line"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "commented_child_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\n\\r]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "commented_children"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "normal_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "token_chain"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "children"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "children": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "line"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "token_chain": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_token"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " "
                },
                {
                  "type": "SYMBOL",
                  "name": "token_chain"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_token": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "component_name"
        },
        {
          "type": "SYMBOL",
          "name": "binding_operator"
        },
        {
          "type": "SYMBOL",
          "name": "typed_list"
        },
        {
          "type": "SYMBOL",
          "name": "typed_dict"
        },
        {
          "type": "SYMBOL",
          "name": "list_marker"
        },
        {
          "type": "SYMBOL",
          "name": "dict_marker"
        },
        {
          "type": "SYMBOL",
          "name": "localized_string"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "identifier_with_modifier"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "component_name": {
      "type": "PATTERN",
      "value": "\\$[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "binding_operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "<=>"
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": "=>"
        }
      ]
    },
    "comment_marker": {
      "type": "STRING",
      "value": "-"
    },
    "typed_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "SYMBOL",
          "name": "component_name"
        }
      ]
    },
    "typed_dict": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "*"
            },
            {
              "type": "STRING",
              "value": "^"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "component_name"
        }
      ]
    },
    "list_marker": {
      "type": "STRING",
      "value": "/"
    },
    "dict_marker": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "^"
        }
      ]
    },
    "localized_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "string_literal": {
      "type": "PATTERN",
      "value": "\\\\[^\\n\\r]*"
    },
    "identifier_with_modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "property_modifier"
        }
      ]
    },
    "property_modifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "?"
        },
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[\\p{L}\\p{Emoji}_][\\p{L}\\p{Emoji}\\p{N}_]*",
      "flags": "u"
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[+-]?\\d+\\.\\d+"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?\\d+"
        },
        {
          "type": "STRING",
          "value": "NaN"
        },
        {
          "type": "STRING",
          "value": "Infinity"
        },
        {
          "type": "STRING",
          "value": "-Infinity"
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "null": {
      "type": "STRING",
      "value": "null"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\t"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    },
    {
      "type": "SYMBOL",
      "name": "_newline"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}