{
	"name": "view-tree-lsp",
	"displayName": "view.tree LSP",
	"description": "Language Server Protocol implementation for view.tree format",
	"version": "0.1.3",
	"license": "MIT",
	"author": "Dev-cmyser",
	"homepage": "https://github.com/Dev-cmyser/view.tree#readme",
	"bugs": {
		"url": "https://github.com/Dev-cmyser/view.tree/issues"
	},
	"main": "./out/vscode/extension.js",
	"bin": {
		"view-tree-lsp": "bin/view-tree-lsp"
	},
	"engines": {
		"vscode": "^1.76.0",
		"node": ">=18.0.0"
	},
	"os": [
		"darwin",
		"linux",
		"win32",
		"win"
	],
	"activationEvents": [
		"onLanguage:view.tree"
	],
	"contributes": {
		"languages": [
			{
				"id": "view.tree",
				"aliases": [
					"view.tree"
				],
				"extensions": [
					".view.tree"
				],
				"configuration": "./.vscode/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "view.tree",
				"scopeName": "source.viewtree",
				"path": "./highlights/TextMate/tmLanguage.json"
			}
		]
	},
	"scripts": {
		"build": "tsc -p ts-config.json",
		"start-server": "node --watch  out/server/index.js --stdio",
		"watch-server": " tsc -w -p ts-config.json"
	},
	"keywords": [
		"view.tree",
		"tree",
		"mol",
		"lsp",
		"language-server",
		"language-server-protocol"
	],
	"dependencies": {
		"mol_tree2": "^1.0.1231",
		"vscode-languageclient": "^9.0.1",
		"vscode-languageserver": "^9.0.1",
		"vscode-languageserver-textdocument": "^1.0.12"
	},
	"devDependencies": {
		"@types/node": "^24.3.1",
		"@types/vscode": "^1.76.0",
		"typescript": "^5.9.2"
	},
	"publisher": "devcmyser",
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dev-cmyser/view.tree.git"
	},
	"files": [
		"bin/",
		"out/server/",
		"README.md",
		"LICENSE"
	]
}
