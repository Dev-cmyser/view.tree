                    },
                    {
                      "type": "SYMBOL",
                      "name": "string_literal"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "typed_list"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "dash_end": {
      "type": "STRING",
      "value": "-"
