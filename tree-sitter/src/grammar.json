{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "molviewtree",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "component_def"
          },
          {
            "type": "SYMBOL",
            "name": "remark_line"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "indent": {
      "type": "PATTERN",
      "value": "\\t+"
