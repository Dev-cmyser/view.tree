        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "indented_node"
          }
        }
      ]
    },
    "indented_node": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "indent"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "property_line"
            },
            {
              "type": "SYMBOL",
              "name": "subcomponent_line"
            },
            {
              "type": "SYMBOL",
              "name": "string_line"
            },
            {
              "type": "SYMBOL",
              "name": "caret_line"
            },
            {
              "type": "SYMBOL",
