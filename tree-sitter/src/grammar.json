    },
    "remark_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "localized_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "PATTERN",
          "value": " +"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "primitive"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "localized_string"
        },
        {
          "type": "SYMBOL",
          "name": "component_name"
        },
        {
          "type": "SYMBOL",
