    "component_name": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "$"
          },
          {
            "type": "PATTERN",
            "value": "[A-Za-z_][A-Za-z0-9_]*"
          }
        ]
      }
    },
    "css_var": {
      "type": "PATTERN",
      "value": "--[A-Za-z0-9_-]+"
    },
    "ident": {
      "type": "PATTERN",
      "value": "[A-Za-z_$][A-Za-z0-9_$-]*"
    },
    "prop_suffix": {
      "type": "PATTERN",
      "value": "[*!?]+"
    },
    "property_id": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "css_var"
              },
              {
                "type": "PATTERN",
                "value": "[A-Za-z_$][A-Za-z0-9_$-]*"
              }
            ]
