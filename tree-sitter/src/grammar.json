          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "prop_suffix"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[+\\-]?(?:NaN|Infinity|\\d+(?:\\.\\d+)?(?:[eE][+\\-]?\\d+)?)"
      }
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "null_kw": {
      "type": "STRING",
      "value": "null"
    },
