        }
      ]
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "string_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "list_marker": {
      "type": "STRING",
      "value": "/"
    },
    "list_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "component_name"
        },
        {
          "type": "STRING",
          "value": "string"
        },
        {
          "type": "STRING",
          "value": "number"
        },
        {
          "type": "STRING",
          "value": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "ident"
        }
      ]
    },
    "dict_marker": {
      "type": "STRING",
      "value": "*"
    },
    "typed_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "list_marker"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "list_type"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "caret_line": {
      "type": "STRING",
